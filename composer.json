{
    "name": "anthony-mills/flashcards",
    "type": "project",
    "description": "Laravel Flash Card application intended to help learning and memory retention.",
    "keywords": [
        "education",
	"learning",
        "laravel"
    ],
    "license": "GPL3",
    "require": {
        "php": "^8.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0.3",
        "laravel/framework": "^9.42.2",
        "laravel/tinker": "^2.7.3",
        "laravel/ui": "^4.1.0",
        "laravelcollective/html": "^6.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13",
        "laravel/dusk": "^7.1.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.1",
        "orangehill/iseed": "^3.0.2",
        "overtrue/phplint": "^5.0.0",
        "phpunit/phpunit": "^9.0.0",
        "vimeo/psalm": "^5.4.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
